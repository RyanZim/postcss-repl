{
  "name": "postcss-repl",
  "version": "1.0.0",
  "private": true,
  "description": "Postcss REPL",
  "homepage": "https://github.com/RyanZim/postcss-repl#readme",
  "bugs": {
    "url": "https://github.com/RyanZim/postcss-repl/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanZim/postcss-repl.git"
  },
  "license": "ISC",
  "author": "Ryan Zimmerman",
  "scripts": {
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json dist",
    "prebuild": "rimraf dist && mkdirp dist && node build-plugins-list.js",
    "build": "run-p build:*",
    "build-production": "cross-env NODE_ENV=production npm run build",
    "build:html-css": "node build-html-css.js",
    "build:js": "webpack --progress",
    "eslint": "eslint . --ignore-path .prettierignore",
    "format": "npm run eslint -- --fix && npm run prettier -- --write && sort-package-json",
    "test": "npm run eslint && npm run prettier -- --list-different && npm run build-production",
    "prettier": "run-s \"prettier:* -- {@}\" --",
    "prettier:normal": "prettier \"**/*.{js,md,css}\"",
    "prettier:svelte": "prettier --parser vue \"**/*.svelte.html\"",
    "prewatch": "npm run build",
    "watch": "run-p watch:*",
    "watch:html-css": "onchange app/index.html app/index.css -v -- npm run build:html-css",
    "watch:js": "webpack --watch",
    "watch:serve": "live-server dist"
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "autoprefixer": "9.1.2",
    "brace": "^0.11.1",
    "browserslist": "^4.0.1",
    "caniuse-lite": "^1.0.30000874",
    "codemirror": "^5.39.2",
    "cssnano-browser": "4.0.0",
    "lodash.debounce": "^4.0.8",
    "lost": "8.3.0",
    "postcss": "^7.0.2",
    "postcss-advanced-variables": "2.3.3",
    "postcss-calc": "6.0.1",
    "postcss-filter-plugins": "3.0.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-mixins": "6.2.0",
    "postcss-nested": "4.0.0",
    "postcss-nesting": "6.0.0",
    "postcss-ordered-values": "4.1.0",
    "postcss-plugins": "^1.12.1",
    "postcss-preset-env": "5.3.0",
    "postcss-short": "4.1.0",
    "postcss-simple-vars": "5.0.1",
    "rucksack-css": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "cross-env": "^5.1.4",
    "cssnano": "^4.0.5",
    "eslint": "^5.2.0",
    "eslint-config-problems": "^1.1.0",
    "live-server": "^1.2.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "null-loader": "^0.1.1",
    "onchange": "^4.1.0",
    "postcss-import": "^12.0.0",
    "postcss-normalize": "^7.0.0",
    "prettier": "1.14.1",
    "rimraf": "^2.6.2",
    "sort-package-json": "^1.11.0",
    "svelte": "^2.9.9",
    "svelte-loader": "^2.10.1",
    "webpack": "^4.16.4",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^3.1.0"
  },
  "browserslist": [
    "> 0.5%"
  ]
}
